[state_vector]
U32 state @alias S
U64 time @alias G
F64 altitude
F64 verticalVelocity
bool enableEngine
bool popNosecone
bool deployParachute
bool ventOpen

[Countdown]
.step
    T == 10: -> Ascent

[Ascent]
.entry
    enableEngine = true
.step
    T == 5: enableEngine = false
    verticalVelocity < 0: -> Descent
.exit
    popNosecone = true

[Descent]
.step
    ventOpen = (5 <= T < 10)
    altitude <= 100: deployParachute = true
    altitude == 0: -> Touchdown

[Touchdown]
